# Uncomment this line to define a global platform for your project
# platform :ios, '9.0'

target 'Brunchly' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Brunchly
	pod 'SwiftyJSON'
	pod 'Alamofire', '~> 4.4'
	pod 'AlamofireImage', '~> 3.1'
	pod 'Kingfisher'
	pod 'ChameleonFramework'
	pod 'Material', '~> 2.0'
	pod 'Firebase/Auth'
	pod 'Firebase/Database'
	pod 'Firebase/Storage'
	pod 'FBSDKCoreKit'
	pod 'FBSDKLoginKit'
	pod 'DZNEmptyDataSet'
	pod 'PDTSimpleCalendar'
	pod 'SideMenu'
	pod 'SlackTextViewController'
	pod 'Spring', :git => 'https://github.com/MengTo/Spring.git', :branch => 'swift3'
	pod 'Koloda', '~> 4.0' # tinder-style swiping
	pod 'Toaster', '~> 2.0' # displaying android-style toasts
	pod 'IQKeyboardManagerSwift'
	pod 'SwiftValidators' # validating forms
	pod 'LocationPicker' # picking location from map or search
	pod "CZPicker" # pop-up option menu
	pod 'ALCameraViewController' # photo picker
	pod 'Hero' # animations
	pod "Floaty", "~> 3.0.0" #floating action button
	pod 'RAMAnimatedTabBarController', "~> 2.0.13"  #tab bar icon animations
end
